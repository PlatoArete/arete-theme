{
  "name": "Arete Theme",
  "type": "dark",
  "semanticHighlighting": true,
  "colors": {
    "activityBar.background": "#0f0d0e",
    "activityBar.foreground": "#d4af37",
    "activityBarBadge.background": "#d4af37",
    "activityBarBadge.foreground": "#252526",
    "badge.background": "#d4af37",
    "badge.foreground": "#2d1b69",
    "breadcrumb.background": "#1e1e1e",
    "breadcrumb.foreground": "#cccccc",
    "breadcrumbPicker.background": "#252526",
    "button.background": "#d4af37",
    "button.foreground": "#2d1b69",
    "button.hoverBackground": "#ffd700",
    "dropdown.background": "#3c3c3c",
    "dropdown.foreground": "#cccccc",
    "editor.background": "#110f10",
    "editor.foreground": "#d4d4d4",
    "editorCursor.foreground": "#d4af37",
    "editorGroup.border": "#444444",
    "editorGroupHeader.tabsBackground": "#0f0d0e",
    "editorIndentGuide.background": "#404040",
    "editorLineNumber.activeForeground": "#d4af37",
    "editorLineNumber.foreground": "#858585",
    "editorWidget.background": "#252526",
    "input.background": "#3c3c3c",
    "input.foreground": "#cccccc",
    "list.activeSelectionBackground": "#d4af37",
    "list.activeSelectionForeground": "#2d1b69",
    "list.hoverBackground": "#2a2d2e",
    "panel.background": "#110f10",
    "panel.border": "#444444",
    "sideBar.background": "#110f10",
    "sideBar.foreground": "#d4af37",
    "sideBarSectionHeader.background": "#2d2d30",
    "sideBarSectionHeader.foreground": "#ffd700",
    "statusBar.background": "#252526",
    "statusBar.foreground": "#d4af37",
    "tab.activeBackground": "#110f10",
    "tab.activeForeground": "#d4af37",
    "tab.activeBorder": "#d4af37",
    "tab.border": "#2d2d30",
    "tab.inactiveBackground": "#0f0d0e",
    "tab.inactiveForeground": "#969696",
    "terminal.ansiBlack": "#000000",
    "terminal.ansiBlue": "#ff8c42",
    "terminal.ansiBrightBlue": "#ffa500",
    "terminal.ansiBrightCyan": "#ffd700",
    "terminal.ansiBrightGreen": "#90ee90",
    "terminal.ansiBrightMagenta": "#ff69b4",
    "terminal.ansiBrightRed": "#ff6347",
    "terminal.ansiBrightWhite": "#ffffff",
    "terminal.ansiBrightYellow": "#ffff00",
    "terminal.ansiCyan": "#ffcc66",
    "terminal.ansiGreen": "#98fb98",
    "terminal.ansiMagenta": "#dda0dd",
    "terminal.ansiRed": "#ff6b47",
    "terminal.ansiWhite": "#d4d4d4",
    "terminal.ansiYellow": "#ffd700",
    "titleBar.activeBackground": "#0f0d0e",
    "titleBar.activeForeground": "#d4af37"
  },
  "tokenColors": [
    {
      "scope": [
        "comment",
        "punctuation.definition.comment"
      ],
      "settings": {
        "foreground": "#9d7cd8",
        "fontStyle": "italic"
      }
    },
    {
      "scope": [
        "constant",
        "entity.name.constant",
        "variable.other.constant",
        "variable.language",
        "constant.character.entity"
      ],
      "settings": {
        "foreground": "#8cc8ff"
      }
    },
    {
      "scope": [
        "entity",
        "entity.name"
      ],
      "settings": {
        "foreground": "#00d2d3"
      }
    },
    {
      "scope": [
        "variable.parameter.function"
      ],
      "settings": {
        "foreground": "#d4d4d4"
      }
    },
    {
      "scope": [
        "entity.name.tag"
      ],
      "settings": {
        "foreground": "#ff4757"
      }
    },
    {
      "scope": [
        "keyword"
      ],
      "settings": {
        "foreground": "#ffcc02",
        "fontStyle": "bold"
      }
    },
    {
      "scope": [
        "storage",
        "storage.type"
      ],
      "settings": {
        "foreground": "#ffcc02",
        "fontStyle": "bold"
      }
    },
    {
      "scope": [
        "storage.modifier.package",
        "storage.modifier.import",
        "storage.type.java"
      ],
      "settings": {
        "foreground": "#d4d4d4"
      }
    },
    {
      "scope": [
        "string",
        "punctuation.definition.string",
        "string punctuation.section.embedded source"
      ],
      "settings": {
        "foreground": "#00bfff"
      }
    },
    {
      "scope": [
        "support"
      ],
      "settings": {
        "foreground": "#ffa502"
      }
    },
    {
      "scope": [
        "meta.property-name"
      ],
      "settings": {
        "foreground": "#ffa502"
      }
    },
    {
      "scope": [
        "variable"
      ],
      "settings": {
        "foreground": "#ff6b6b"
      }
    },
    {
      "scope": [
        "variable.other"
      ],
      "settings": {
        "foreground": "#ff6b6b"
      }
    },
    {
      "scope": [
        "invalid.broken"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#ff4500"
      }
    },
    {
      "scope": [
        "invalid.deprecated"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#ff4500"
      }
    },
    {
      "scope": [
        "invalid.illegal"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#ff0000"
      }
    },
    {
      "scope": [
        "invalid.unimplemented"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#ff4500"
      }
    },
    {
      "scope": [
        "carriage-return"
      ],
      "settings": {
        "background": "#ff0000",
        "content": "^M",
        "fontStyle": "italic underline"
      }
    },
    {
      "scope": [
        "message.error"
      ],
      "settings": {
        "foreground": "#ff0000"
      }
    },
    {
      "scope": [
        "string source"
      ],
      "settings": {
        "foreground": "#d4d4d4"
      }
    },
    {
      "scope": [
        "string variable"
      ],
      "settings": {
        "foreground": "#ffcc66"
      }
    },
    {
      "scope": [
        "source.regexp",
        "string.regexp"
      ],
      "settings": {
        "foreground": "#ff8c42"
      }
    },
    {
      "scope": [
        "string.regexp.character-class",
        "string.regexp constant.character.escape",
        "string.regexp source.ruby.embedded",
        "string.regexp string.regexp.arbitrary-repitition"
      ],
      "settings": {
        "foreground": "#ffd700"
      }
    },
    {
      "scope": [
        "string.regexp constant.character.escape"
      ],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#ff7f7f"
      }
    },
    {
      "scope": [
        "support.constant"
      ],
      "settings": {
        "foreground": "#ff7f7f"
      }
    },
    {
      "scope": [
        "support.variable"
      ],
      "settings": {
        "foreground": "#ff8c42"
      }
    },
    {
      "scope": [
        "meta.module-reference"
      ],
      "settings": {
        "foreground": "#ff8c42"
      }
    },
    {
      "scope": [
        "punctuation.definition.list.begin.markdown"
      ],
      "settings": {
        "foreground": "#ffa500"
      }
    },
    {
      "scope": [
        "markup.heading",
        "markup.heading entity.name"
      ],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#ff6347"
      }
    },
    {
      "scope": [
        "markup.quote"
      ],
      "settings": {
        "foreground": "#ff7f7f"
      }
    },
    {
      "scope": [
        "markup.italic"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#d4d4d4"
      }
    },
    {
      "scope": [
        "markup.bold"
      ],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#d4d4d4"
      }
    },
    {
      "scope": [
        "markup.raw"
      ],
      "settings": {
        "foreground": "#ff8c42"
      }
    },
    {
      "scope": [
        "markup.deleted",
        "meta.diff.header.from-file",
        "punctuation.definition.deleted"
      ],
      "settings": {
        "background": "#ffdddd",
        "foreground": "#ff0000"
      }
    },
    {
      "scope": [
        "markup.inserted",
        "meta.diff.header.to-file",
        "punctuation.definition.inserted"
      ],
      "settings": {
        "background": "#ddffdd",
        "foreground": "#90ee90"
      }
    },
    {
      "scope": [
        "markup.changed",
        "punctuation.definition.changed"
      ],
      "settings": {
        "background": "#ffffdd",
        "foreground": "#ffd700"
      }
    },
    {
      "scope": [
        "markup.ignored",
        "markup.untracked"
      ],
      "settings": {
        "foreground": "#2f363d",
        "background": "#ff8c42"
      }
    },
    {
      "scope": "meta.diff.range",
      "settings": {
        "foreground": "#ff8c42",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "meta.diff.header",
      "settings": {
        "foreground": "#ff8c42"
      }
    },
    {
      "scope": "meta.separator",
      "settings": {
        "fontStyle": "bold",
        "foreground": "#ff8c42"
      }
    },
    {
      "scope": "meta.output",
      "settings": {
        "foreground": "#ff8c42"
      }
    },
    {
      "scope": [
        "brackethighlighter.tag",
        "brackethighlighter.curly",
        "brackethighlighter.round",
        "brackethighlighter.square",
        "brackethighlighter.angle",
        "brackethighlighter.quote"
      ],
      "settings": {
        "foreground": "#858585"
      }
    },
    {
      "scope": "brackethighlighter.unmatched",
      "settings": {
        "foreground": "#ff0000"
      }
    },
    {
      "scope": [
        "constant.other.reference.link",
        "string.other.link"
      ],
      "settings": {
        "foreground": "#ffd700",
        "fontStyle": "underline"
      }
    },
    {
      "scope": [
        "keyword.control.sas",
        "keyword.other.sas"
      ],
      "settings": {
        "foreground": "#ff6b47",
        "fontStyle": "bold"
      }
    },
    {
      "scope": [
        "entity.name.function.sas",
        "support.function.sas"
      ],
      "settings": {
        "foreground": "#ffa500"
      }
    },
    {
      "scope": [
        "string.quoted.single.sas",
        "string.quoted.double.sas"
      ],
      "settings": {
        "foreground": "#ffd700"
      }
    },
    {
      "scope": [
        "variable.other.sas",
        "entity.name.variable.sas"
      ],
      "settings": {
        "foreground": "#ffcc66"
      }
    },
    {
      "scope": [
        "comment.line.sas",
        "comment.block.sas"
      ],
      "settings": {
        "foreground": "#9d7cd8",
        "fontStyle": "italic"
      }
    }
  ]
}